{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users/.*",
        "dest": "backend/server.js"
      },
      {
        "src": "/api/loans/.*",
        "dest": "backend/server.js"
      }
    ]
  }
  